<!DOCTYPE html>
<%= render "/refinery/html_tag" %>
  <% site_bar = render('/refinery/site_bar', :head => true) -%>
  <head>
    <%= render "/refinery/head" %>
    <%= render "/refinery/javascripts" %>
  </head>
  <body id="<%= canonical_id @page %>" class="header-fixed header-fixed-space">
    <%= site_bar -%>

    <div id="page_container" class="wrapper">
      <header id="header" class="header-v6 header-classic-white header-sticky">
        <%= render "/refinery/header" -%>
      </header>
      <section id="page" class="container content-md">
        <%= yield %>
      </section>
      <footer class="footer-v1">
        <%= render "/refinery/footer" -%>
      </footer>
    </div>

  </body>
</html>
